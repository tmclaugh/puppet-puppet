# $Id$
# XXX: THis should be a template.
pid_file=/var/run/nrpe.pid
server_port=5666
allowed_hosts=nagios.straycat.dhs.org
nrpe_user=nagios
nrpe_group=nagios
dont_blame_nrpe=0
debug=0
command_timeout=60

command[check_users]=/usr/lib64/nagios/plugins/check_users -w 5 -c 25
command[check_load]=/usr/lib64/nagios/plugins/check_load -w 15,10,5 -c 30,25,20
command[check_hda1]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 8% -p /dev/hda1
command[check_disk_root]=/usr/lib64/nagios/plugins/check_disk -w 20 -c 8 -p /
command[check_zombie_procs]=/usr/lib64/nagios/plugins/check_procs -w 5 -c 10 -s Z
command[check_total_procs]=/usr/lib64/nagios/plugins/check_procs -w 150 -c 200
command[check_mem]=/usr/lib64/nagios/plugins/check_swap -w 90 -c 85

